<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/report/courserequestor/db" VERSION="20110209" xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance" COMMENT="tables for course requestor"
    xsi:noNamespaceSchemaLocation="../../lib/xmldb/xmldb.xsd">
	<TABLES>
	<TABLE NAME="ucla_request_classes" COMMENT="Table to request and track course builds" NEXT="ucla_request_crosslist">
      <FIELDS>
      <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="term"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="srs"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="term" NEXT="course"/>
        <FIELD NAME="course" TYPE="char" LENGTH="20" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="srs" NEXT="department"/>
        <FIELD NAME="department" TYPE="char" LENGTH="20" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="course" NEXT="instructor"/>
        <FIELD NAME="instructor" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="department" NEXT="contact"/>
        <FIELD NAME="contact" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="instructor" NEXT="crosslist"/>
        <FIELD NAME="crosslist" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="contact" NEXT="added_at"/>
		<FIELD NAME="added_at" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="crosslist" NEXT="action"/>
        <FIELD NAME="action" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="added_at" NEXT="status"/>
        <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="action" NEXT="mailinst"/>
        <FIELD NAME="mailinst" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" ENUM="false"  PREVIOUS="status" NEXT="hidden"/>
		<FIELD NAME="hidden" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false"  PREVIOUS="mailinst" NEXT="force_urlupdate"/>
		<FIELD NAME="force_urlupdate" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="hidden" NEXT="force_no_urlupdate"/>
		<FIELD NAME="force_no_urlupdate" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="force_urlupdate"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="ucla_request_crosslist" COMMENT="Table that holds the list of crosslisted classes (srs list)" PREVIOUS="ucla_request_classes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="term"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="srs"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="term" NEXT="aliassrs"/>
        <FIELD NAME="aliassrs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="srs" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="aliassrs"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>	
  </TABLES>
</XMLDB>